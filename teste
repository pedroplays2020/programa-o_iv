from enum import Enum
class imparpar(Enum):
    PAR=0
    IMPAR=1
def checkpar(list,p):
  
    lista=[]
    for num in list:
        if num%2==p:
            lista.append(num)
    return lista
def converter(text):
    lista2=[]
    lista=text.split(",")
    for n in lista:
        lista2.append(int(n))
    return tuple(checkpar(lista2,imparpar.PAR.value))

def mult(nums,multiplayer):
    arr=[]
    for n in nums:
        if n%multiplayer==0:
            arr.append(n)
    return arr

print(converter(input("digite um tuplo: ")))
